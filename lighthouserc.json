{
  "ci": {
    "collect": {
      "staticDistDir": "./docs",
      "url": [
        "/",
        "/blog/",
        "/work/",
        "/about/",
        "/404.html"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {}
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./.lighthouseci"
    }
  }
}
